{
    "name": "yeganehha/laravel-dynamic-form",
    "description": "Create different forms for each part of the site, such as registration, which can be edited by the admin.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Yeganehha\\DynamicForm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yeganehha\\DynamicForm\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yeganehha\\DynamicForm\\DynamicFormServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Erfan Ebrahimi",
            "email": "me@erfanebrahimi.ir",
            "homepage": "http://erfanebrahimi.ir/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^8.75",
        "livewire/livewire": "^2.10",
        "wire-elements/modal": "^1.0"
    },
    "keywords": [
        "Laravel",
        "Form",
        "DynamicForm",
        "Dynamic Form",
        "Dynamic",
        "Fileds",
        "Dynamic Fields",
        "DynamicFields"
    ]
}
